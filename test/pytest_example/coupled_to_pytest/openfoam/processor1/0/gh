FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    location    "0";
    object      gh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -2 0 0 0 0];

internalField   uniform (0 -9.81 0);

boundaryField
{
    top
    {
        type            fixedValue;
        value           nonuniform List<vector> 0();
    }
    bottom
    {
        type            fixedValue;
        value           uniform (0 -9.81 0);
    }
    streamwiseIn
    {
        type            cyclic;
    }
    streamwiseOut
    {
        type            cyclic;
    }
    front
    {
        type            cyclic;
    }
    back
    {
        type            cyclic;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform (0 -9.81 0);
    }
    procBoundary1to0throughstreamwiseOut
    {
        type            processorCyclic;
        value           uniform (0 -9.81 0);
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform (0 -9.81 0);
    }
}


